<form method="POST" action="/auth/signup">
  <h1>CREATE YOUR ACCOUNT!!</h1>
  <div>
    <label>Username</label>
    <input type="text" name="username"/>
  </div>

  <div>
    <label>Password</label>
    <input type="password" name="password"/>
  </div>

  <div>
    <button type="submit">Submit</button>
  </div>
</form>